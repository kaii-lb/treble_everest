From 8b653b699e58c5aec1310c1b84374504a685da6c Mon Sep 17 00:00:00 2001
From: "Christopher A. Williamson" <home@chrisaw.com>
Date: Sat, 17 Feb 2024 17:14:15 +0000
Subject: [PATCH] Add missing ref to
 sdmcore_has_is_display_hw_available_func_defaults

---
 build/soong/Android.bp | 17 +++++++++++++++++
 1 file changed, 17 insertions(+)

diff --git a/build/soong/Android.bp b/build/soong/Android.bp
index 448cae0..bd33a03 100644
--- a/build/soong/Android.bp
+++ b/build/soong/Android.bp
@@ -67,6 +67,23 @@ soong_config_module_type {
     properties: ["cppflags"],
 }
 
+soong_config_module_type {
+    name: "sdmcore_has_is_display_hw_available",
+    module_type: "cc_defaults",
+    config_namespace: "voltageGlobalVars",
+    bool_variables: ["sdmcore_has_is_display_hw_available_func"],
+    properties: ["cflags"],
+}
+
+sdmcore_has_is_display_hw_available {
+    name: "sdmcore_has_is_display_hw_available_func_defaults",
+    soong_config_variables: {
+        sdmcore_has_is_display_hw_available_func: {
+            cflags: ["-DSDMCORE_HAS_IS_DISPLAY_HW_AVAILABLE_FUNC"],
+        },
+    },
+}
+
 camera_override_format_from_reserved {
     name: "camera_override_format_from_reserved_defaults",
     soong_config_variables: {
